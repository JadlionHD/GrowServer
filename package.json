{
  "name": "growserver",
  "version": "2.4.0",
  "description": "a growtopia private server",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run lint && swc ./src -d dist",
    "start": "node -r dotenv/config dist/src/app.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "lint": "eslint lint ./src",
    "iteminfo:fetch": "node wiki-scraper/scrape.js",
    "dev": "rimraf dist && rimraf assets/cache && eslint ./src && (swc ./src -d dist || tsc) && node -r dotenv/config dist/src/app.js",
    "dev:nolint": "rimraf dist && rimraf assets/cache && (swc ./src -d dist || tsc) && node -r dotenv/config dist/src/app.js",
    "install": "(node scripts/setup.js) && npm run migrate && npm run seed && npm run finish",
    "finish": "echo \u001b[46mSetup Completed!\u001b[0m && echo \u001b[46mCheck out GrowServer github: https://github.com/JadlionHD/GrowServer\u001b[0m"
  },
  "author": "JadlionHD <jadlion.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@swc/cli": "^0.3.0",
    "@swc/core": "^1.3.105",
    "@types/chance": "^1.1.6",
    "@types/crypto-js": "^4.1.1",
    "@types/decompress": "^4.2.7",
    "@types/express": "^4.17.21",
    "@types/node": "^18.11.9",
    "@types/ws": "^8.5.10",
    "eslint": "^9.2.0",
    "globals": "^15.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "better-sqlite3": "^9.2.2",
    "chalk": "4.1.1",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.1.1",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "growtopia.js": "^1.3.6",
    "knex": "^2.5.1",
    "ws": "^8.17.0"
  },
  "packageManager": "pnpm@9.1.1+sha256.9551e803dcb7a1839fdf5416153a844060c7bce013218ce823410532504ac10b"
}
